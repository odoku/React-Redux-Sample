{
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-react"
  ],
  "rules": {
    "no-console": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "interface-name": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules",
      "public/assets/js/*",
      "**/*.d.ts"
    ]
  }
}
